    "@chainsafe/common-theme": "1.0.10",
    "@chainsafe/web3-context": "1.2.0",
    "@hookform/resolvers": "^2.8.4",
    "@material-ui/styles": "4.10.0",
    "@mui/icons-material": "^5.2.1",
    "@mui/lab": "^5.0.0-alpha.57",
