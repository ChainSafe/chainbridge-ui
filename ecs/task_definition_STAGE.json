{
  "containerDefinitions": [ 
     { 
        "essential": true,
        "image": "852551629426.dkr.ecr.us-east-2.amazonaws.com/bridge-ui-stage:STAGE",
        "logConfiguration": { 
           "logDriver": "awslogs",
           "options": { 
              "awslogs-group": "/ecs/bridge-ui-STAGE",
              "awslogs-region": "us-east-2",
              "awslogs-stream-prefix": "ecs"
           }
        },
        "name": "bridge-ui-container-STAGE",
        "portMappings": [ 
           { 
              "containerPort": 80,
              "hostPort": 80,
              "protocol": "tcp"
           }
        ],
        "environment": [
         {
            "name": "example",
            "value": "setanenv"
          }
        ],
        "secrets": [ 
           { 
              "name": "mychainbridgesecret",
              "valueFrom": "/chainbridge/example"
           }
        ]
     }
  ],
  "cpu": "256",
  "executionRoleArn": "arn:aws:iam::852551629426:role/bridge-ui-ecsTaskExecutionRole",
  "family": "bridge-ui-container-STAGE",
  "memory": "512",
  "networkMode": "awsvpc",
  "runtimePlatform": {
       "operatingSystemFamily": "LINUX"
   },
  "requiresCompatibilities": [ 
      "FARGATE" 
   ]
}
